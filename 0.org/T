{
    inlet
    {
        type            fixedValue;
	value		uniform 293;
    }

    outlet
    {
        type            zeroGradient;
    }

    solidWall
    {
        type            zeroGradient;
    }
    
    frontAndBack
    {
        type            zeroGradient;
    }
